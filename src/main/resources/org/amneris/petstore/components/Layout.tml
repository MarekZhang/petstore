	<link rel="author" href="humans.txt" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0"/>

	<!-- jQuery Visualize Styles -->
	<link rel='stylesheet' type='text/css' href='${context:huraga/css/plugins/jquery.visualize.css}'/>

	<!-- CSS styles -->
	<link rel='stylesheet' type='text/css' href='${context:huraga/css/huraga-dark.css}'/>
	<link rel='stylesheet' type='text/css' href='${context:huraga-t5patches.css}'/>

