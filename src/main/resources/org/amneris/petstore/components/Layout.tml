	<script src="${context:huraga/js/libs/modernizr.js}" type="text/javascript"></script>
	<script src="${context:huraga/js/libs/selectivizr.js}" type="text/javascript"></script>

	<script type="text/javascript">
		$(document).ready(function(){

			// Tooltips
			$('[title]').tooltip({
				placement: 'top'
			});

		});
	</script>

</head>

<body id="${bodyId}">

<header class="container">

	<!-- Main page logo -->
	<h1><a href="/" class="_brand" style="margin-left:-4px">Tynamo Pet Store</a></h1>

	<!-- Main page headline -->
	<p>Tynamo is model-driven, full-stack web framework based on Apache Tapestry 5</p>

	<!-- Alternative navigation -->
	<nav>
		<ul>
			<li>
				<form class="nav-search">
					<input type="text" placeholder="Search&hellip;"/>
				</form>
			</li>
			<li>
				<div class="btn-group">
					<a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
						Configuration
						<span class="caret"></span>
					</a>
					<ul class="dropdown-menu">
						<li><a href="401.html"><span class="awe-flag"></span> Error page 401</a></li>
						<li><a href="403.html"><span class="awe-flag"></span> Error page 403</a></li>
						<li><a href="404.html"><span class="awe-flag"></span> Error page 404</a></li>
						<li><a href="500.html"><span class="awe-flag"></span> Error page 500</a></li>
						<li><a href="503.html"><span class="awe-flag"></span> Error page 503</a></li>
					</ul>
				</div>
			</li>
			<li><t:security.loginlink/></li>
		</ul>
	</nav>
	<!-- /Alternative navigation -->

</header>


<!-- Main page container -->
<section class="container" role="main">

	<!-- Left (navigation) side -->
	<div class="navigation-block">

		<!-- User profile -->
		<section class="user-profile">
			<figure class="clearfix">
				<img alt="John Pixel avatar" src="http://placekitten.com/60/60"/>
				<figcaption>
					<strong><a href="#" class="">John Pixel</a></strong>
					<em>marketing manager</em>
					<ul>
						<li><a class="btn btn-primary btn-flat" href="#" title="Account settings">settings</a></li>
						<li><a class="btn btn-primary btn-flat" href="#" title="Message inbox">inbox</a></li>
					</ul>
				</figcaption>
			</figure>
		</section>
		<!-- /User profile -->

		<!-- Sample left search bar -->
		<form class="side-search">
			<input type="text" class="rounded" placeholder="To search type and hit enter"/>
		</form>
		<!-- /Sample left search bar -->

		<!-- Main navigation -->
		<nav class="main-navigation" role="navigation">
			<ul>
				<li t:type="any" t:mixins="nav" nav.page="dashboard" nav.class="current"><t:pagelink class="no-submenu" page="dashboard"><span class="awe-home"></span>Dashboard</t:pagelink></li>
				<li><a href="forms.html" class="no-submenu"><span class="awe-tasks"></span>Forms</a></li>
				<li><a href="charts.html" class="no-submenu"><span class="awe-signal"></span>Charts <span class="badge">7</span></a></li>
				<li t:type="any" t:mixins="nav" nav.expression="/((list|show|add|edit)/)?(mydomainobject|user)(/.*)?" nav.class="current">
					<a href="#">
						<span class="awe-table"></span>Other Entities <span class="badge">2</span></a>
					<t:tynamo.ListPageLinkList listPageName="List"/>
				</li>
			</ul>
		</nav>
		<!-- /Main navigation -->

		<!-- Sample side note -->
		<section class="side-note">
			<div class="side-note-container">
				<h2>Sample Side Note</h2>
				<p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Phasellus quis erat dui, quis purus.</p>
			</div>
			<div class="side-note-bottom"></div>
		</section>
		<!-- /Sample side note -->

	</div>
	<!-- Left (navigation) side -->

	<!-- Right (content) side -->
	<div class="content-block" role="main">
		<t:body/>
	</div>

</section>
<!-- /Main page container -->

	<!-- Main page footer -->
	<footer class="container">
		<p>Powered by <a href="http://tynamo.org/">Tynamo</a>. Developed by <a href="http://www.amneris.eu/">Amneris Labs</a></p>
		<ul>
		</ul>
		<a href="#top" class="btn btn-primary btn-flat pull-right">Top &uarr;</a>
	</footer>

	<!-- Scripts -->
	<script src="${context:/huraga/js/navigation.js}"></script>
	<script src="${context:/huraga/js/bootstrap/bootstrap-affix.js}"></script>
	<script src="${context:/huraga/js/bootstrap/bootstrap-tooltip.js}"></script>
	<script src="${context:/huraga/js/bootstrap/bootstrap-collapse.js}"></script>
	<script src="${context:/huraga/js/bootstrap/bootstrap-dropdown.js}"></script>

<div id="ajax-indicator" style="display:none;" class="loading" data-original-title="Loading, please waitâ€¦">Loading...</div>

